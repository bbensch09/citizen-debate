<p id="notice"><%= notice %></p>

<h1>Listing Topic Votes</h1>

<table>
  <thead>
    <tr>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @topic_votes.each do |topic_vote| %>
      <tr>
        <td><%= topic_vote.value %></td>
        <td><%= link_to 'Show', topic_vote %></td>
        <td><%= link_to 'Edit', edit_topic_vote_path(topic_vote) %></td>
        <td><%= link_to 'Destroy', topic_vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Topic vote', new_topic_vote_path %>
