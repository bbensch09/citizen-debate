<div class="profile-container">

<h1>Listing Available Times</h1>

<table class="col-lg-12 col-sm-6 waitlist-table">
  <thead>
    <tr>
      <th>Debate</th>
      <th>Proposed by</th>
      <th>Proposed to</th>
      <th>Proposed time</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @available_times.each do |available_time| %>
      <tr>
        <td><%= available_time.debate_id %></td>
        <td><%= available_time.proposed_by %></td>
        <td><%= available_time.proposed_to %></td>
        <td><%= available_time.proposed_time %></td>
        <td><%= available_time.status %></td>
        <td><%= link_to 'Show', available_time %></td>
        <td><%= link_to 'Edit', edit_available_time_path(available_time) %></td>
        <td><%= link_to 'Destroy', available_time, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Available time', new_available_time_path %>
</div>
