<p id="notice"><%= notice %></p>

<h1>Listing Topic Tags</h1>

<table>
  <thead>
    <tr>
      <th>Debate</th>
      <th>Topic</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @topic_tags.each do |topic_tag| %>
      <tr>
        <td><%= topic_tag.debate_id %></td>
        <td><%= topic_tag.topic_id %></td>
        <td><%= link_to 'Show', topic_tag %></td>
        <td><%= link_to 'Edit', edit_topic_tag_path(topic_tag) %></td>
        <td><%= link_to 'Destroy', topic_tag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Topic tag', new_topic_tag_path %>
